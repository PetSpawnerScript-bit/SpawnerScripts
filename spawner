-- Detect Delta and block usage
local exploit = identifyexecutor and identifyexecutor() or "Unknown"

if string.lower(exploit):find("delta") then
    game:GetService("StarterGui"):SetCore("SendNotification", {
        Title = "Executor Blocked",
        Text = "Delta not supported.",
        Duration = 5
    })
    return -- Stops the script from continuing
end

-- Your actual script loader below
loadstring(game:HttpGet("https://raw.githubusercontent.com/PetSpawnerScript-bit/SpawnerScripts/main/spawner.lua"))()
